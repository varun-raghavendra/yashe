    "from sympy.polys.domains import ZZ\n",
    "from sympy.polys.galoistools import gf_gcdex\n",
    "\n",
    "p = ZZ.map([4, 4, 96, 97])\n",
    "q = ZZ.map([1, 0, 1])\n",
    "z = 93\n",
    "s, t, g = gf_gcdex(p, q, z, ZZ)\n",
    "if len(g) == 1 and g[0] == 1: \n",
    "    print(s)\n",
    "else:\n",
    "    print('no inverse')"
